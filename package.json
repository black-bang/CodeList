{
  "name": "ReactWebpack",
  "version": "1.0.0",
  "description": "快速搭建基于webpack的React开发环境",
  "main": "",
  "scripts": {
    "dev": "set NODE_ENV=development&&webpack-dev-server --port 8084 --open http://localhost:8084/ --inline --config ./webpack.config/webpack.dev.config.js",
    "test": "set NODE_ENV=production&&webpack --config ./webpack.config/webpack.test.config.js && node ./TestServer/TestServer.js",
    "assets": "set NODE_ENV=production&&webpack --config ./webpack.config/webpack.assets.config.js"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-1",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      [
        "import",
        [
          {
            "libraryName": "antd-mobile",
            "style": "css"
          }
        ]
      ]
    ]
  },
  "author": "ysc",
  "license": "ISC",
  "devDependencies": {
    "alloyfinger": "^0.1.15",
    "antd-mobile": "^2.1.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-mobx": "^1.0.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chart.js": "^2.7.2",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "css-loader": "^0.28.11",
    "date": "^1.0.2",
    "date-format": "^1.2.0",
    "draft-js": "^0.10.5",
    "echarts": "^4.1.0",
    "encodeurl": "^1.0.2",
    "es6-promise": "^4.2.2",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.5",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "js-cookie": "^2.2.0",
    "json-loader": "^0.5.7",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "prop-types": "^15.6.2",
    "pubsub-js": "^1.6.1",
    "react": "^16.4.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bmap": "^1.0.80",
    "react-dom": "^16.4.1",
    "react-draft-wysiwyg": "^1.12.13",
    "react-hammerjs": "^1.0.1",
